{
	"useTabs": false,
	"tabWidth": 2,
	"arrowParens": "always",
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 80,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"semi": true,
  "bracketSpacing": true
}
